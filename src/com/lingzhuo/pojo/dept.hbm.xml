<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	
	<hibernate-mapping>
	<class name="com.lingzhuo.entity.DeptEntity" table="dept" schema="scott">
	<id name="deptno" column="deptno" type="java.lang.Integer">
	<generator class="assigned"></generator>
	</id>
	<property name="dname" column="dname" type="java.lang.String"></property>
	<property name="loc" column="loc" type="java.lang.String"></property>
	</class>
	
	
	</hibernate-mapping>